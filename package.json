{
  "name": "colonel-kurtz",
  "version": "2.1.1",
  "description": "A block editor",
  "main": "build/colonel-kurtz.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --content-base ./example --config webpack.example.config.js",
    "prepublish": "NODE_ENV=production webpack -p --optimize-occurence-order --optimize-dedupe",
    "test": "NO_COVERAGE=true karma start",
    "test:coverage": "CONTINUOUS_INTEGRATION=true npm test",
    "coveralls": "CONTINUOUS_INTEGRATION=true npm test && coveralls < coverage/report-lcov/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vigetlabs/colonel-kurtz.git"
  },
  "keywords": [
    "block",
    "editor"
  ],
  "contributors": [
    {
      "name": "Lawson Kurtz",
      "email": "lawson.kurtz@viget.com",
      "url": "http://viget.com"
    },
    {
      "name": "Nate Hunzaker",
      "email": "nate.hunzaker@viget.com",
      "url": "http://viget.com"
    }
  ],
  "license": "MIT",
  "bugs": "https://github.com/vigetlabs/colonel-kurtz/issues",
  "homepage": "https://github.com/vigetlabs/colonel-kurtz",
  "devDependencies": {
    "autoprefixer-core": "^5.1.8",
    "babel-loader": "^5.x.x",
    "babel-runtime": "^5.x.x",
    "coveralls": "^2.11.2",
    "css-loader": "^0.9.0",
    "css-mqpacker": "^3.0.1",
    "csswring": "^3.0.1",
    "extract-text-webpack-plugin": "^0.5.0",
    "istanbul": "^0.3.13",
    "istanbul-instrumenter-loader": "^0.1.2",
    "json-loader": "^0.5.1",
    "karma": "^0.12.31",
    "karma-chrome-launcher": "^0.1.7",
    "karma-cli": "0.0.4",
    "karma-coverage": "^0.2.7",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.1.10",
    "karma-nyan-reporter": "0.0.60",
    "karma-sinon-chai": "^0.3.0",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.0",
    "mocha": "^2.1.0",
    "postcss-loader": "^0.3.0",
    "raw-loader": "^0.5.1",
    "react": "^0.13.1",
