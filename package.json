{
  "name": "colonel-kurtz",
  "version": "0.0.3",
  "description": "A block editor",
  "main": "build/colonel-kurtz.js",
  "scripts": {
    "start": "NODE_ENV=development webpack -wd --config webpack.example.config.js",
    "prepublish": "NODE_ENV=production webpack -p --optimize-occurence-order --optimize-dedupe",
    "test": "jest colonel-kurtz"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/vigetlabs/colonel-kurtz.git"
  },
  "keywords": [
    "block",
    "editor"
  ],
  "contributors": [
    {
      "name": "Lawson Kurtz",
      "email": "lawson.kurtz@viget.com",
      "url": "http://viget.com"
    },
    {
      "name": "Nate Hunzaker",
      "email": "nate.hunzaker@viget.com",
      "url": "http://viget.com"
    }
  ],
  "jest": {
    "scriptPreprocessor": "<rootDir>/lib/jestPreprocessor.js",
    "setupEnvScriptFile": "<rootDir>/lib/jestSetup.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/object-assign",
      "<rootDir>/node_modules/array.prototype.find"
    ],
    "globals": {
      "__DEV__": true
    }
  },
  "license": "MIT",
  "bugs": "https://github.com/vigetlabs/colonel-kurtz/issues",
  "homepage": "https://github.com/vigetlabs/colonel-kurtz",
  "devDependencies": {
    "autoprefixer-loader": "^1.0.0",
    "css-loader": "^0.9.0",
    "envify-loader": "^0.1.0",
    "react-dragon": "^0.0.2",
    "extract-text-webpack-plugin": "^0.3.5",
    "jest-cli": "^0.2.1",
    "json-loader": "^0.5.1",
    "jsx-loader": "^0.12.2",
    "react-tools": "^0.12.2",
    "sass-loader": "^0.3.1",
    "style-loader": "^0.8.2",
    "webpack": "^1.4.14"
  },
  "peerDependencies": {
    "array.prototype.find": "*",
    "object-assign": "*",
    "flux": "*",
    "react": ">= 0.12.1"
  }
}
